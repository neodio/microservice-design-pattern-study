  "properties": {
    "version": { "type": "string" },
    "orderId": { "type": "string" },
    "productId": { "type": "string" },
    "qty": { "type": "integer" },
    "totalPrice": { "type": "number" },
    "simulateCancel": { "type": "boolean" }
  }
}